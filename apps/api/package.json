{
  "name": "api",
  "version": "1.0.0",
  "description": "DocIntel Pro API - NestJS Lambda Backend",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build && node esbuild.config.js",
    "start": "node dist/main.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .nest build"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.716.0",
    "@aws-sdk/client-s3": "^3.716.0",
    "@aws-sdk/client-textract": "^3.716.0",
    "@docintel/shared": "workspace:*",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "pdfjs-dist": "^4.9.155",
    "pino": "^9.5.0",
    "pino-pretty": "^12.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "esbuild": "^0.24.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
